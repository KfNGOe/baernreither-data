                           <ref name="anchor"/>
                           <ref name="note"/>
                           <ref name="hi"/>
                           <ref name="choice"/>
                           <ref name="add"/>
                           <ref name="del"/>
                           <ref name="addSpan"/>
                           <ref name="app"/>
