   <define name="addSpan">
      <element name="addSpan">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(added span of text) marks the beginning of a longer sequence of text added by an author, scribe, annotator or corrector.</a:documentation>
         <empty/>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.spanning.attribute.spanTo"/>
         <ref name="att.typed.attribute.type"/>
      </element>
   </define>
   
   <define name="app">
      <element name="app">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(apparatus entry) contains one entry in a critical apparatus.</a:documentation>
         <group>
