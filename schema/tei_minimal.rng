   <define name="add">
      <element name="add">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(addition) contains letters, words, or phrases inserted in the source text by an author, scribe, or a previous annotator or corrector.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="pb"/>
               <ref name="anchor"/>
               <ref name="note"/>
               <ref name="hi"/>
               <ref name="choice"/>
               <ref name="add"/>
               <ref name="del"/>
               <ref name="addSpan"/>
               <ref name="app"/>
               <ref name="rdg"/>
               <ref name="ref"/>
               <ref name="persName"/>
               <ref name="orgName"/>
               <ref name="placeName"/>
               <ref name="term"/>
               <ref name="abbr"/>
